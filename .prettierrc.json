{
	"plugins": [
		"prettier-plugin-tailwindcss"
	],
	"tailwindStylesheet": "./assets/css/main.css",
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"jsxSingleQuote": false,
	"printWidth": 100,
	"semi": false,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true,
	"vueIndentScriptAndStyle": true
}